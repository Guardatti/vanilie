{
    "version": 2,
    "builds": [
        {
        "src": "backend/app.ts",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/products(.*)",
            "dest": "backend/app.ts"
        },
        {
            "src": "/auth(.*)",
            "dest": "backend/app.ts"
        },
        {
            "src": "/orders(.*)",
            "dest": "backend/app.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}