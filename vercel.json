{
    "version": 2,
    "builds": [
        {
        "src": "backend/app.ts",
        "use": "@vercel/node"
        },
        {
        "src": "vanilie/dist/**/*",
        "use": "@vercel/static"
        }
    ],
    "routes": [
        {
        "src": "/api/(.*)",
        "dest": "backend/app.ts"
        },
        {
        "src": "/(.*)",
        "dest": "vanilie/dist/$1"
        }
    ]
}